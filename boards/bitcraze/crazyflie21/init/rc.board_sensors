#!/bin/sh
#
# Bitcraze Crazyflie specific board sensors init
#------------------------------------------------------------------------------

board_adc start

# Internal SPI BMI088
bmi088 -A -R 2 -s start
bmi088 -G -R 2 -s start

# Internal I2C bus
bmp388 -I start

# Optical flow deck
vl53l0x start -X
