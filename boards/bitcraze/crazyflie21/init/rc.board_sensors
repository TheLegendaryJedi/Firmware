#!/bin/sh
#
# Bitcraze Crazyflie specific board sensors init
#------------------------------------------------------------------------------

board_adc start

# Internal SPI BMI088
#bmi088 -A -R 10 -s start
bmi088 -G -R 10 -s start

# Internal I2C bus
bmp388 -I -a 0x77 start
bmp388 -I start
bmi088 -A -R 10 -I -a 0x77 start

# Optical flow deck
vl53lxx start -X
